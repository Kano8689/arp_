SELECT m._id, m.faculty_id, m.course_id, m.semester_year, m.semester_type, f.faculty_name AS faculty_name, c.course_code AS course_code, c.course_name AS course_name FROM mapping_faculty m LEFT JOIN faculties_details f ON m.faculty_id = f._id LEFT JOIN courses_table c ON m.course_id = c._id GROUP BY m.faculty_id, m.course_id, m.semester_year, m.semester_type ORDER BY m.created_at DESC LIMIT 0, 10;

SELECT m._id, m.faculty_id, m.course_id, m.semester_year, m.semester_type, f.faculty_name AS faculty_name, c.course_code AS course_code, c.course_name AS course_name FROM mapping_faculty m LEFT JOIN faculties_details f ON m.faculty_id = f._id LEFT JOIN courses_table c ON m.course_id = c._id m.semester_year = '2025' AND m.semester_type='1' GROUP BY m.faculty_id, m.course_id, m.semester_year, m.semester_type ORDER BY m.created_at DESC LIMIT 0, 10

SELECT m._id, m.faculty_id, m.course_id, m.semester_year, m.semester_type, f.faculty_name AS faculty_name, c.course_code AS course_code, c.course_name AS course_name FROM mapping_faculty m LEFT JOIN faculties_details f ON m.faculty_id = f._id LEFT JOIN courses_table c ON m.course_id = c._id m.semester_year = '2025' AND m.semester_type='1' GROUP BY m.faculty_id, m.course_id, m.semester_year, m.semester_type ORDER BY m.created_at DESC LIMIT 0, 10